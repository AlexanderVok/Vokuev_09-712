#include "pch.h"
#include <iostream>
#include <omp.h>
#include <stdio.h>
using namespace std;
int main()
{
#pragma omp parallel num_threads(3) 
	if (omp_get_num_threads() > 1)
	{
		printf("Thread number %d, number of threads %d\n", omp_get_thread_num(), omp_get_num_threads());
	}
#pragma omp parallel num_threads(1)
	if (omp_get_num_threads() > 1)
	{
		printf("Thread number %d, number of threads %d\n", omp_get_thread_num(), omp_get_num_threads());
	}
}
